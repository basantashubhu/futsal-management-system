{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": "^7.1.3",
		"ext-json": "*",
		"barryvdh/laravel-cors": "^0.11.3",
		"doctrine/dbal": "^2.7",
		"fideloper/proxy": "^4.0",
		"laravel/framework": "5.6.*",
		"laravel/passport": "^6.0",
		"laravel/tinker": "^1.0",
		"orangehill/iseed": "^2.5",
		"phpoffice/phpspreadsheet": "^1.7",
		"predis/predis": "^1.1",
		"smalot/pdfparser": "^0.12.0",
		"tymon/jwt-auth": "dev-develop"
	},
	"require-dev": {
		"filp/whoops": "^2.0",
		"fzaninotto/faker": "^1.4",
		"mockery/mockery": "^1.0",
		"nunomaduro/collision": "^2.0",
		"phpunit/phpunit": "^7.0",
		"setasign/fpdi-fpdf": "^2.0"
	},
	"autoload": {
		"classmap": [
			"database/seeds",
			"database/factories"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"files": [
			"app/Helpers/validation/valid.php",
			"app/Helpers/file/file.php",
			"app/Helpers/date/date.php",
			"app/Helpers/customHelper.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		},
		"files": [
			"tests/Utils/functions.php"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
